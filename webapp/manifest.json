{
  "_version": "1.12.0",
  "sap.app": {
    "id": "ESGOrg.ESGOrg",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "ESGOrg.ESGOrg.view.Login",
      "type": "XML",
      "async": true,
      "id": "Login"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ESGOrg.ESGOrg.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ESGOrg.ESGOrg.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Login",
          "pattern": "Login",
          "target": "Login"
        },
        {
          "name": "EmployeeLogin",
          "pattern": "EmployeeLogin",
          "target": "EmployeeLogin"
        },
        {
          "name": "Main",
          "pattern": "Main",
          "target": "Main"
        },
        {
          "name": "EnvironmentAnalytics",
          "pattern": "EnvironmentAnalytics",
          "target": "EnvironmentAnalytics"
        },
        {
          "name": "EmmissionsReporting",
          "pattern": "EmmissionsReporting",
          "target": "EmmissionsReporting"
        },
        {
          "name": "FuelBioRef",
          "pattern": "FuelBioRef",
          "target": "FuelBioRef"
        },
        {
          "name": "SocialReporting",
          "pattern": "SocialReporting",
          "target": "SocialReporting"
        },
        {
          "name": "GovernanceReporting",
          "pattern": "GovernanceReporting",
          "target": "GovernanceReporting"
        },
        {
          "name": "ElecHeatCooling",
          "pattern": "ElecHeatCooling",
          "target": "ElecHeatCooling"
        },
        {
          "name": "Employement",
          "pattern": "Employement",
          "target": "Employement"
        },
        {
          "name": "Leave",
          "pattern": "Leave",
          "target": "Leave"
        },
        {
          "name": "Entity",
          "pattern": "Entity",
          "target": "Entity"
        },
        {
          "name": "EcoPerformance",
          "pattern": "EcoPerformance",
          "target": "EcoPerformance"
        }
      ],
      "targets": {
        "Login": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Login"
        },
        "EmployeeLogin": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "EmployeeLogin"
        },
        "Main": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Main"
        },
        "EnvironmentAnalytics": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "EnvironmentAnalytics"
        },
        "EmmissionsReporting": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "EmmissionsReporting"
        },
        "FuelBioRef": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Emissions/FuelBioRef"
        },
        "SocialReporting": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "SocialReporting"
        },
        "GovernanceReporting": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "GovernanceReporting"
        },
        "ElecHeatCooling": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Emissions/ElecHeatCooling"
        },
        "Employement": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Social/Employement"
        },
        "Leave": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Social/Leave"
        },
        "Entity": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Governance/Entity"
        },
        "EcoPerformance": {
          "viewType": "XML",
          "transition": "slide",
          "viewName": "Governance/EcoPerformance"
        }
      }
    }
  }
}