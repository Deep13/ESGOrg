<mvc:View controllerName="ESGOrg.ESGOrg.controller.ReportingTiles" xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true"
    xmlns:object="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m">
    <Page id="page" titleAlignment="Center" class="sapUiSizeCompact">
        <customHeader>
            <OverflowToolbar>
                <Button icon="sap-icon://navigation-left-arrow" tooltip="Back" press="onpressBack" />
                <Image src="./assets/company_logo.png" class="sapUiMediumMargin"
                    width="60px" />
                <ToolbarSpacer />
                <Title id="title" text="" />
                <ToolbarSpacer />
                <HBox alignItems="Center">
                    <Button icon="sap-icon://log" tooltip="Log Out" press="onLogOut" />
                    <Button icon="sap-icon://headset" tooltip="Support" press="" />
                </HBox>
            </OverflowToolbar>
        </customHeader>
        <content>
            <Panel id="tilePanel" width="auto" class="sapUiResponsiveMargin" accessibleRole="Region">
                <headerToolbar>
                    <OverflowToolbar>
                        <Title text="Reporting" />
                    </OverflowToolbar>
                </headerToolbar>
                <content>


                </content>
            </Panel>
        </content>
        <footer>
            <OverflowToolbar>
                <ToolbarSpacer />
                <Text text="Koshish Sustainable Solutions Pvt.Ltd Â© 2024" />
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>